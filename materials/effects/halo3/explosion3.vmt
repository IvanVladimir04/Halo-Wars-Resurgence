"UnlitGeneric"
{
	"$basetexture"	"effects/halo3/fire_cloud2"
	"$additive"	1
	"$vertexcolor"	1
	"$vertexalpha"	1
	"$translucent" 				"1"

"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "45"
      }
   }

}