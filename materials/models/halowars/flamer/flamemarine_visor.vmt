"VertexLitGeneric"
{
	"$baseTexture" "models/halowars/flamer/flamemarine_01_df"
	"$bumpmap" "models/halowars/flamer/flamemarine_visor_01_nm"
	"$envmap" "models/halowars/shared/cyborg_reflection_armor.hdr"
	"$normalmapalphaenvmapmask"	"1"
	"$envmaptint"	"{185 135 5}"
	"$multipass" 1
	"$envmapcontrast"		"0.45"
	"$envmapsaturation "	"0.85"
	"$halflambert" "1"
	"$nodecal" "1"


	"$blendtintbybasealpha"	"1"

	Proxies
	{
		PlayerColor
		{
			resultVar	$color2 // pass the player color value to Gmod
			default		0.23 0.35 0.41 // unused for now.
		}
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}

}
