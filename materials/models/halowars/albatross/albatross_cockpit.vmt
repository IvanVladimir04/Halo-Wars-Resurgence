"VertexLitGeneric"
{
	"$baseTexture" "models/halowars/albatross/albatross_01_df"
	"$bumpmap" "models/halowars/albatross/albatross_cockpit_01_nm"
	"$envmap" "models/halowars/shared/cyborg_reflection_armor.hdr"
	"$normalmapalphaenvmapmask"	"1"
	"$envmaptint"	"{125 65 15}"
	"$multipass" 1
	"$envmapcontrast"		"0.5"
	"$envmapsaturation "	"0.25"
	"$phong" 1
	"$phongboost" "0.85"
	"$phongexponent" "7"
	"$phongtint" "{230 255 215}"
	"$phongfresnelranges" "[.7 .82 31]"
	"$halflambert" "1"
	"$nodecal" "1"
	"$nocull"		"1"
	"$rimlight" 1
	$rimlightboost          .2
	"$rimlightexponent"       7
	"$blendtintbybasealpha"	"1"
//	"$color2"		"[1.5 1.5 1.5]"

	Proxies
	{
		PlayerColor
		{
			resultVar	$color2 // pass the player color value to Gmod
			default		0.23 0.35 0.41 // unused for now.
		}
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}

}
