"VertexLitGeneric"
{
	"$baseTexture" "models/halowars/turret/turretshaft_01_df"
	"$bumpmap" "models/halowars/turret/turretshaft_01_nm"
	"$phong" 1
	"$phongboost" "1.5"
	"$phongexponent" "3"
	"$phongtint" "{230 255 215}"
	"$phongfresnelranges" "[.7 .82 31]"
	"$halflambert" "1"
	"$nodecal" "1"
	"$nocull"		"1"
	"$rimlight" 1
	$rimlightboost          .5
	"$rimlightexponent"       17
	"$blendtintbybasealpha"	"1"

	Proxies
	{
		PlayerColor
		{
			resultVar	$color2 // pass the player color value to Gmod
			default		0.23 0.35 0.41 // unused for now.
		}
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}

}
