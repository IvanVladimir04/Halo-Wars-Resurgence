"VertexLitGeneric"
{
	"$baseTexture" "models/halowars/constructor/buildpod_01_df"
	"$bumpmap" "models/halowars/constructor/buildpod_01_nm"
	"$phong" 1
	"$phongboost" "1.4"
	"$phongexponent" "14"
	"$phongtint" "{230 255 195}"
	"$phongfresnelranges" "[.7 .82 31]"
	"$halflambert" "1"
	"$nodecal" "1"
	"$nocull"		"1"
	"$rimlight" 1
	$rimlightboost          0.15
	"$rimlightexponent"       45
	"$blendtintbybasealpha"	"1"

	Proxies
	{
		PlayerColor
		{
			resultVar	$color2 // pass the player color value to Gmod
			default		0.23 0.35 0.41 // unused for now.
		}
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}

}
