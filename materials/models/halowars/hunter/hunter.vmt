"VertexLitGeneric"
{
	"$baseTexture" "models/halowars/hunter/hunter_01_df"
	"$bumpmap" "models/halowars/hunter/hunter_01_nm"
	"$nocull"		"1"
	"$detail" "models/halowars/hunter/hunter_01_rm"
	"$detailscale" "1"
	"$detailblendfactor" "1"
	"$detailblendmode" "1"

	"$ambientocclusion" "1"
	"$phong" 1
	"$phongboost" "1.8"
	"$phongexponent" "6"
	"$phongtint" "{230 200 255}"
	"$phongfresnelranges" "[.7 .82 31]"
	"$model" 1
	"$phongalbedotint" "1"
	"$blendtintbybasealpha"	"1"
	"$rimlight" 1
	$rimlightboost          .8
	"$rimlightexponent"       15
	Proxies
	{
		PlayerColor
		{
			resultVar	$color2 // pass the player color value to Gmod
			default		0.23 0.35 0.41 // unused for now.
		}
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}
}

