"UnlitGeneric"
{
	"$baseTexture" "models/halowars/hunter/glow"
	"$nocull"		"1"
	"$blendtintbybasealpha"	"1"
	"$color2"		"[0.2 1.2 1.5]"

	Proxies
	{
		PlayerColor
		{
			resultVar	$color2 // pass the player color value to Gmod
			default		0.23 0.35 0.41 // unused for now.
		}
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}
}

