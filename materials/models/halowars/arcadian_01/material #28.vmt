"VertexLitGeneric"
{
	"$baseTexture" "models/halowars/arcadian_01/arcadian04_df"
	"$bumpmap" "models/halowars/police/arcadiapolice_01_nm"
	"$phong" 1
	"$phongboost" "0.85"
	"$phongexponent" "7"
	"$phongtint" "{230 255 215}"
	"$phongfresnelranges" "[.7 .82 31]"
	"$halflambert" "1"
	"$nodecal" "1"
	"$nocull"		"1"

	"$blendtintbybasealpha"	"1"

	Proxies
	{
		Clamp
		{
			min 0.15
			max 1
			srcVar1 "$color"
			resultVar "$phongtint"
		}
		Clamp
		{
			min 0.25
			max 1
			srcVar1 "$color"
			resultVar "$envmaptint"
		}
	}

}
