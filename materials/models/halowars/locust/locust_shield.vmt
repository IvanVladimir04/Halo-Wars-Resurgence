"VertexlitGeneric"
{
	"$additive" 1
//	"$translucent" 1
	"$vertexcolor" 1
//	"$vertexalpha" 1
	"$decal" 1

//	"$alpha" 1	
	"$color2"		"[2 10 15]"
	"$basetexture" "models/halowars/elite\Electric_Field"
	"$selfillum"	"1"
	$emissiveBlendEnabled 1
	$emissiveBlendTexture     "models/halowars/elite\Electric_Field"
	$emissiveBlendBaseTexture		"models/halowars/elite\Electric_Field"
	$emissiveBlendFlowTexture     "models/halowars/elite/particle_glow_04"
